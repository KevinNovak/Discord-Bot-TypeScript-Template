{
    "prefix": "bot",
    "client": {
        "token": "<DISCORD_BOT_TOKEN>",
        "intents": [
            "GUILDS",
            "GUILD_MEMBERS",
            "GUILD_PRESENCES",
            "GUILD_MESSAGES",
            "DIRECT_MESSAGES"
        ],
        "partials": [],
        "caches": {
            "messages": {
                "size": 1,
                "lifetime": 60,
                "sweepInterval": 30
            }
        }
    },
    "jobs": {
        "updateServerCount": {
            "schedule": "0 */5 * * * *"
        }
    },
    "colors": {
        "default": "#0099ff",
        "error": "#ff4a4a"
    },
    "sharding": {
        "machineId": 0,
        "machineCount": 1,
        "serversPerShard": 1000,
        "spawnDelay": 5,
        "spawnTimeout": 300
    },
    "links": {
        "support": "https://discord.com/",
        "stream": "https://www.twitch.tv/monstercat"
    }
}
