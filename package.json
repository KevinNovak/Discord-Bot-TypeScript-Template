{
    "name": "my-bot",
    "version": "1.0.0",
    "author": "Kevin Novak",
    "description": "A TypeScript Discord bot template with multi-machine sharding and other features.",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=12.14.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "lint": "tslint --project .",
        "lint:fix": "tslint --fix --project .",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "build": "tsc",
        "start": "npm run build && node --enable-source-maps dist/start.js",
        "start:shard": "npm run build && node --enable-source-maps dist/app.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "discord.js": "12.4.1",
        "node-fetch": "2.6.1",
        "node-schedule": "1.3.2",
        "pm2": "^4.5.0"
    },
    "devDependencies": {
        "@types/node": "14.14.6",
        "@types/node-fetch": "2.5.7",
        "@types/node-schedule": "1.3.1",
        "prettier": "^2.1.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.5"
    }
}
